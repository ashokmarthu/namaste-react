    "firebase": "^11.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router": "^7.0.1"
  },
  "browserslist": [
    "last 3 versions"
  ]
