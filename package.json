{
  "name": "namaste-react",
  "version": "1.0.0",
  "description": "React Learnings from Akshay Saini",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node"
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashokmarthu/namaste-react.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Ashok Marthu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashokmarthu/namaste-react/issues"
  },
  "homepage": "https://github.com/ashokmarthu/namaste-react#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.6.6",
    "node-fetch": "^2.7.0",
    "parcel": "^2.13.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.4.0",
